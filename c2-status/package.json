{
  "name": "c2-status",
  "version": "1.0.0",
  "description": "A service that returns slave statuses generated by camus2",
  "main": "./dist/server.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
