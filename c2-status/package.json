    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@types/node": "^12.7.5",
    "tsc-watch": "^4.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
